    %load/vec4 v0x600001b0dcb0_0;
    %part/s 1;
    %assign/vec4 v0x600001b0def0_0, 0;
    %load/vec4 v0x600001b0dcb0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x600001b0dcb0_0, 0;
    %pushi/vec4 2, 0, 3;
    %assign/vec4 v0x600001b0e400_0, 0;
    %jmp T_11.7;
T_11.4 ;
    %load/vec4 v0x600001b0de60_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_11.8, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x600001b0ddd0_0, 0;
    %pushi/vec4 3, 0, 3;
    %assign/vec4 v0x600001b0e400_0, 0;
T_11.8 ;
    %jmp T_11.7;
T_11.5 ;
    %load/vec4 v0x600001b0de60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_11.10, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x600001b0ddd0_0, 0;
    %pushi/vec4 1, 0, 3;
    %assign/vec4 v0x600001b0e400_0, 0;
    %load/vec4 v0x600001b0dcb0_0;
    %cmpi/e 32, 0, 32;
    %jmp/0xz  T_11.12, 4;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x600001b0e400_0, 0;
    %load/vec4 v0x600001b0e0a0_0;
    %cmpi/e 12, 0, 32;
