T_11.7 ;
    %pop/vec4 1;
T_11.1 ;
    %jmp T_11;
    .thread T_11;
    .scope S_0x14b706990;
T_12 ;
    %wait E_0x600003c050c0;
    %load/vec4 v0x600001b0dcb0_0;
    %pushi/vec4 12, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x600001b0e1c0_0;
    %load/vec4 v0x600001b0e0a0_0;
    %subi 1, 0, 32;
    %ix/vec4/s 4;
    %load/vec4a v0x600001b0e010, 4;
    %cmp/ne;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_12.0, 8;
    %vpi_call 2 129 "$display", "Failure!" {0 0 0};
    %load/vec4 v0x600001b0e0a0_0;
    %subi 1, 0, 32;
    %ix/vec4/s 4;
    %load/vec4a v0x600001b0e010, 4;
    %vpi_call 2 130 "$display", "DUT Scan code = %x is not equal to expected = %x!", v0x600001b0e1c0_0, S<0,vec4,u8> {1 0 0};
T_12.0 ;
    %jmp T_12;
